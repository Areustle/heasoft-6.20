#!/bin/sh
(TCLTK_LIBRARY=$FTOOLS/lib ;\
export TCLTK_LIBRARY ;\
unset TCL_LIBRARY; unset TK_LIBRARY; unset TCLLIBPATH ;\
LD_LIBRARY_PATH=$TCLTK_LIBRARY:$TCLTK_LIBRARY/itcl:$LD_LIBRARY_PATH ;\
export LD_LIBRARY_PATH ;\
FITSLAUNCHER_LIBRARY=$FTOOLS/lib/flaunch ;\
export FITSLAUNCHER_LIBRARY ;\
EXPECTDIR=$FTOOLS/lib ;\
export EXPECTDIR ;\
LHEA_HELP=$LHEA_HELP ;\
export LHEA_HELP ;\
ITCL_LIBRARY=$FTOOLS/lib/itcl/itcl ;\
export ITCL_LIBRARY ;\
$FTOOLS/bin/wish $FITSLAUNCHER_LIBRARY/flaunch.tcl -name flaunch $1 $2 $3 $4 $5 $6 $7 $8 $9)
