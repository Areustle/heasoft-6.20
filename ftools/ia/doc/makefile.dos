# NMAKE           ! Makes DHFHLP.EXE, HLPDHF.EXE, XHELP.EXE
# NMAKE CLEAN     ! To clean up
#
FFLAGS =
XANLIB = \XANADU\LIB\XANLIB.LIB

#
ALL: DHFHLP.EXE HLPDHF.EXE XHELP.EXE

#
DHFHLP.EXE : DHFHLP.OBJ
      LINK DHFHLP,,NUL,$(XANLIB);

#
HLPDHF.EXE : HLPDHF.OBJ
      LINK HLPDHF,,NUL,$(XANLIB);

#
XHELP.EXE : XHELP.OBJ
      LINK XHELP,,NUL,$(XANLIB);

CLEAN:
      DEL *.OBJ

.SUFFIXES: .FOR .OBJ

.FOR.OBJ:
      FL /c /Gt $(FFLAGS) $<
